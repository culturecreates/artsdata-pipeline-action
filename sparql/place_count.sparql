PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <http://schema.org/>

SELECT (COUNT(?place) AS ?count)
WHERE {
  VALUES ?class { 
    schema:Place 
    schema:PerformingArtsTheater 
    schema:MusicVenue 
    schema:CivicStructure 
    schema:MovieTheater
  }
  ?place a ?class .
}

